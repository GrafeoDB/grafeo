{
  "name": "@grafeo-db/js",
  "version": "0.4.3",
  "description": "Node.js/TypeScript bindings for Grafeo - a high-performance embeddable graph database",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "grafeo",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "graph",
    "database",
    "gql",
    "cypher",
    "sparql",
    "knowledge-graph",
    "native",
    "napi",
    "embedded"
  ],
  "author": "StevenBtw",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/GrafeoDB/grafeo.git",
    "directory": "crates/bindings/node"
  },
  "homepage": "https://grafeo.dev",
  "bugs": {
    "url": "https://github.com/GrafeoDB/grafeo/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --features full",
    "build:debug": "napi build --platform",
    "test": "vitest run",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "vitest": "^4.0.18"
  }
}
